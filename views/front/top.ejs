$(document).ready(function(){
    $('#mob-search1').click(function(){ // ID가 toggleButton인 요소를 클릭하면
        var state = $('.mob-search-box1').css('display'); // state 변수에 ID가 moreMenu인 요소의 display의 속성을 '대입'
        var state2 = $('.mob-search-box-img1').css('display'); // state 변수에 ID가 moreMenu인 요소의 display의 속성을 '대입'
        if(state == 'none' && state2 == 'none'){ // state가 none 상태일경우 
            $('.mob-search-box1').show(); // ID가 moreMenu인 요소를 show();
            $('.mob-search-box-img1').show(); // ID가 moreMenu인 요소를 show();
        }else{ // 그 외에는
            $('.mob-search-box1').hide(); // ID가 moreMenu인 요소를 hide();     
            $('.mob-search-box-img1').hide(); // ID가 moreMenu인 요소를 hide();     
        }
    });    
    
});

function search_submit(){
	if(document.search_box.keyword.value==""){
	 alert("검색어를 입력해 주세요.");
	document.search_box.keyword.focus();
	  return;
	}
	 	document.search_box.submit();
}
