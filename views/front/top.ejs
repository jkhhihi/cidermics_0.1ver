$(document).ready(function(){
$('.contents_menu').click(function(e){
      e.preventDefault();
      if($(this).is('.on')){
         $(this).removeClass('on');
         $('.contents_sub_menu').slideUp();
         $('.contents_menu').text("▼");
      }else{
         $(this).addClass('on');
         $('.contents_sub_menu').slideDown();
         $('.contents_menu').text("▲");
      }
});
});


</script>

