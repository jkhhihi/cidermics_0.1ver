     alert("이메일 주소를 입력해 주세요.");
    document.prinsert.email.focus();
      return;
    }

    if(!document.prinsert.check1.checked){
                alert("서비스 이용약관에 동의하셔야 신청이 가능합니다."); 
                return;
         
        }else if(document.prinsert.check1.checked){
