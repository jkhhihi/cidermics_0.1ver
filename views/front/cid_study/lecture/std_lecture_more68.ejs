<script type="text/javascript">
  function commaNum(num) {
      var len, point, str;  
      num = num + "";  
      point = num.length % 3  
      len = num.length;  
   
      str = num.substring(0, point);  
      while (point < len) {  
          if (str != "") str += ",";  
          str += num.substring(point, point + 3);  
          point += 3;  
      }  
      return str;  
  }


  $(document).ready(function(){    
      for (i=0; i<$(".amount").length; i++){
          $(".amount").eq(i).text(commaNum($(".amount").eq(i).html()));   
      }
  });

</script>

