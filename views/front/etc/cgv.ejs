<style type="text/css">
	/*fab*/
	.fab {
	    z-index: 999;
	    background: #2196F3;
	    width: 50px;
	    height: 50px;
	    border-radius: 50%;
	    text-align: center;
	    color: #FFF;
	    box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.5), 3px 3px 3px rgba(0, 0, 0, 0.25);
	    position: fixed;
	    bottom: 85px;
	    right: 27px;
	    font-size: 3.2rem;
	    display: inline-block;
	    cursor: pointer;
	}
	.fab span {
	    vertical-align: middle;
	}
	.fab span a {
	    line-height: 50px;
	    color: #fff;
	}
	.fab span a:hover{
	    color: #e4dddd;
	    text-decoration: none !important;
	}
	.fab.child {
	    z-index: 999;
	    right: 28px;
	    width: 50px;
	    height: 50px;
	    display: none;
	    opacity: 0;
	    font-size: 14px;
	}
	.backdrop {
	    position: fixed;
	    top: 200px;
	    left: 0;
	    width: 100%;
	    height: 100%;
	    display: none;
	}
    /*fab end*/
</style>

<script type="text/javascript">
 
</script>

<body>	
	<% include ../top %>

    <!--fab-->
    <div class="backdrop" id="backdrop"></div>
    <div class="fab child" data-subitem="1"><span><a class="" href="/seminar" onclick="ga('send','event','세미나','버튼클릭','detail페이지' );">세미나</a></span></div>
    <div class="fab child" data-subitem="2"><span><a class="" href="/study" onclick="ga('send','event','강의','버튼클릭','detail페이지' );">강의</a></span></div>
    <div class="fab child" data-subitem="3"><span><a class="" href="https://finance.cidermics.com/finance" onclick="ga('send','event','컨설팅','버튼클릭','detail페이지' );">컨설팅</a></span></div>
    <div class="fab child" data-subitem="4" style="background: #fff; padding:2px"><a href="http://bit.ly/2JK1TQI"  onclick="ga('send','event','CGV','CGV버튼클릭','CGV페이지' );"><img class="img-responsive" src="/page_imgs/main_img/cgvlogo.svg" style="height: 46px"></a></div>

    <div class="fab" id="masterfab"><span id="clickfab" onclick='var pcode="<i class=material-icons>add</i>"; var mcode="<i class=material-icons>close</i>"; if(backdrop.style.display=="none"){clickfab.innerHTML = mcode;}else{clickfab.innerHTML = pcode; }'><i class='material-icons'>add</i></span></div>
	<script>
	    $(function(){
	        $(".backdrop").fadeIn(125);
	        $(".fab.child").each(function(){
	            $(this)
	                .stop()
	                .show()
	                .animate({
	                    bottom	: (parseInt($("#masterfab").css("bottom")) + parseInt($("#masterfab").outerHeight()) + 60 * $(this).data("subitem") - $(".fab.child").outerHeight()) + "px",
	                    opacity	: 1
	                },125);
	        });
	        $(".fab,.backdrop").click(function(){
	            if($(".backdrop").is(":visible")){
	                $(".backdrop").fadeOut(125);
	                $(".fab.child")
	                    .stop()
	                    .animate({
	                        bottom	: $("#masterfab").css("bottom"),
	                        opacity	: 0
	                    },125,function(){
	                        $(this).hide();
	                    });
	            }else{
	                $(".backdrop").fadeIn(125);
	                $(".fab.child").each(function(){
	                    $(this)
	                        .stop()
	                        .show()
	                        .animate({
	                            bottom	: (parseInt($("#masterfab").css("bottom")) + parseInt($("#masterfab").outerHeight()) + 60 * $(this).data("subitem") - $(".fab.child").outerHeight()) + "px",
	                            opacity	: 1
	                        },125);
	                });
	            }
	        });
	    });

	</script>

	<div class="container">
		<div class="row">
			<div class="col-12">
				<img src="https://i.imgur.com/Tnw8uM1.jpg">
			</div>
		</div>
	</div>




	<script type="text/javascript" src="/js/exentriq-bootstrap-material-ui.js"></script>


	<% include ../bottom %>
</body>
