</head>
<body>
<!--위로가기 버튼-->
<div id="but_top"><img class="gototop" src="/page_imgs/fixed_img/btn_gotoup.svg"></div>
<% include ../top %>

<div class="bg-blue">
<div class="bg-white main-contents container">

<div class="contents-menu">
	<div class="container">
		<div class="menu" align="center">
			<a href="/contents"><img src="/page_imgs/contents_img/click_contents_menu1.svg" height='80'></a>
			<a href="/contents/1"><img src="/page_imgs/contents_img/contents_menu2.svg" height='60'></a>
			<a href="/contents/2"><img src="/page_imgs/contents_img/contents_menu3.svg" height='60'></a>
			<a href="/contents/3"><img src="/page_imgs/contents_img/contents_menu4.svg" height='60'></a>
			<a href="/contents/4"><img src="/page_imgs/contents_img/contents_menu5.svg" height='60'></a>
		</div>
	</div>
</div>

<div class="contents-mob_menu">
		<div class="menu row" align="center">
				<div class="col-xs-12 btn" style="padding:0; margin:0 0 15px 0;">
						<a href="/contents"><img src="/page_imgs/contents_img/click_icon_menu_mob1.svg" width='60'></a>
						<a href="/contents/1"><img src="/page_imgs/contents_img/icon_menu_mob2.svg" width='60'></a>
						<a href="/contents/2"><img src="/page_imgs/contents_img/icon_menu_mob3.svg" width='60'></a>
						<a href="/contents/3"><img src="/page_imgs/contents_img/icon_menu_mob4.svg" width='60'></a>
						<a href="/contents/4"><img src="/page_imgs/contents_img/icon_menu_mob5.svg" width='60'></a>
				</div>
		</div>
</div>



<!--콘텐츠뿌림-->
<div class="contents container" style="margin-bottom:50px;">
	<div class="popular-title" align="center">
	<p>콘텐츠 인기 TOP 30</p>
	</div>
			<ul class="grid effect-2" id="grid">
				
				<% for(var i=0; i<contents.length; i++) { %>
				<li>
				<div>
					
					<a href="/contents/detail/<%= contents[i].con_no %>">
					<div class="con-img" align="center"><img class="img-responsive" src="<%= contents[i].con_photo %>"></div></a>
					
					<p><a href="/contents/detail/<%= contents[i].con_no %>">
						<%= contents[i].con_title %></a></p>
				</div>
			</li>
			<% } %>
			</ul>
</div>

</div>
</div>

		<script src="/js/contents_js/masonry.pkgd.min.js"></script>
		<script src="/js/contents_js/imagesloaded.js"></script>
		<script src="/js/contents_js/classie.js"></script>
		<script src="/js/contents_js/AnimOnScroll.js"></script>
		<script>
			new AnimOnScroll( document.getElementById( 'grid' ), {
				minDuration : 0.4,
				maxDuration : 0.7,
				viewportFactor : 0.2
			} );
		</script>


<% include ../bottom %>


